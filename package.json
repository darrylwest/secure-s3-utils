{
  "name": "s3-utils",
  "version": "0.1.0",
  "description": "A CLI wrapper around secure-s3-store for encrypted S3 file operations",
  "main": "dist/cli.js",
  "bin": {
    "s3-utils": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "put": "node dist/cli.js put",
    "get": "node dist/cli.js get"
  },
  "keywords": ["s3", "cli", "encryption", "secure-s3-store", "devops"],
  "author": "dpw",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "secure-s3-store": "git+https://github.com/darrylwest/secure-s3-store.git#v1.0.6",
    "winston": "^3.11.0",
    "@dotenvx/dotenvx": "^1.0.0"
  }
}